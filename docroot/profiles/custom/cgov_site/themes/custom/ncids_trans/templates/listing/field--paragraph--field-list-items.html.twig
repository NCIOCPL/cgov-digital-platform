{% if items | length > 0 %}
  {% set style = element['#object'].field_list_item_style.value %}
  {% if style starts with 'ncids' %}
    {% set attr = create_attribute({'class': ['usa-collection']}) %}
  {% endif %}
  <ul{{ attr }}>
    {% for item in items %}
      {{ item.content }}
    {% endfor %}
  </ul>
{% endif %}
