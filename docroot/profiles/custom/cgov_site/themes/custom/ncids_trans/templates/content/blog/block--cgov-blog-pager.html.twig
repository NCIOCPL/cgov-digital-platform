{% set catch_cache = content|render %}
<div id='cgov-blog-post-pagination' class='clearfix'>

  {% if content['#cgov_block_data'].prev_nid is not empty %}
    <div class='blog-post-older'>
      <a href="{{ content['#cgov_block_data'].prev_link }}">&lt; {% trans %}Older Post{% endtrans %}</a>
      <p><i>{{ content['#cgov_block_data'].prev_title }}</i></p>
    </div>
  {% endif %}

  {% if content['#cgov_block_data'].next_nid is not empty %}
    <div class='blog-post-newer'>
      <a href="{{ content['#cgov_block_data'].next_link }}">{% trans %}Newer Post{% endtrans %} &gt;</a>
      <p><i>{{ content['#cgov_block_data'].next_title }}</i></p>
    </div>
  {% endif %}

</div>