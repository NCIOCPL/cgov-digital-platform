{#
/**
 * @file
 * Default theme implementation to display a app_module.
 *
 * Available variables:
 * - app_module_plugin_id: The ID of the app module implementation.
 * - configuration: A list of the app module's configuration values.
 * - content: The content of this app_module screen.
 * - attributes: array of HTML attributes populated by modules, intended to
 *   be added to the main container tag of this template.
 *   - id: A valid HTML ID and guaranteed unique.
 *
 * @see template_preprocess_app_module()
 *
 * @ingroup themeable
 */
#}
{{ attach_library('ncids_trans/r4r-app') }}

<noscript>You need to enable JavaScript to run this app.</noscript>
<div id="NCI-R4R-root"></div>
<div class="r4r-page-options-container">
	{{ drupal_block('page_options') }}
</div>
<script type="text/javascript">
	window.R4RAppSettings = {{ configuration|json_encode|raw }}
</script>
