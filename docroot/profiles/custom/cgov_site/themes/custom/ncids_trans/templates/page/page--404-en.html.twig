{{ page.ncids_header }}

<div id="page" class="grid-container">
  <div class="main-content" id="content">
    <div class="general-page-body-container collapse">
      {{ page.ncids_breadcrumb }}

      <div class="grid-row grid-gap">
        {% if page.ncids_sidenav %}
          <!-- SECTION NAVIGATION -->
          <div class="desktop:grid-col-3">
            {{ page.ncids_sidenav }}
          </div>
        {% endif %}
        <div class="{% if page.ncids_sidenav %}desktop:grid-col-9{% else %}grid-col-12{% endif %}">
          <div class="legacy">
            <!-- Begin MAIN CONTENT AREA -->
            <main class="columns contentzone has-section-nav" id="main-content">
              <div class="error-page">
                {{ page.page_not_found }}
              </div>
            </main>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

{{ page.ncids_footer }}
