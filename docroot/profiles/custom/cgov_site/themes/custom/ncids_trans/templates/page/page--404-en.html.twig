      <div class="row general-page-body-container collapse">
        <div class="large-12 columns">
          <div class="row">
            {{ page.breadcrumb }}
          </div> <!-- END "row" -->
        </div> <!-- END "large-12 columns" -->
        <div class="row">
          <!-- SECTION NAVIGATION -->
          {% if page.ncids_sidenav %}
            {{ page.ncids_sidenav }}
          {% endif %}
          <div class="medium-9 columns contentzone has-section-nav" id="main" tabindex="0" role="main">
            <div class="error-page">
              {{ page.page_not_found }}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

