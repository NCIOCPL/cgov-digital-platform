{
  "name": "ncids_trans",
  "version": "1.0.0",
  "description": "Front-end package for ncids_trans theme.",
  "main": "index.js",
  "scripts": {
    "clean:dist": "rimraf ../dist",
    "clean:legacy-sprite": "rimraf ./build && rimraf ./src/styles/legacy-sprites/_svg-sprite-map.scss",
    "clean:all": "npm run clean:dist && npm run clean:legacy-sprite",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "npm run build:legacy-sprite && webpack --mode development",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "npm run build:legacy-sprite && webpack --mode production",
    "prewatch": "npm run clean:dist",
    "watch": "npm run build:legacy-sprite && webpack watch --mode development",
    "prebuild:legacy-sprite": "npm run clean:legacy-sprite",
    "build:legacy-sprite": "node ./bin/build-sprites.js",
    "lint": "eslint -c ./.eslintrc.js --no-eslintrc src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint -c ./.eslintrc.js --no-eslintrc src --fix --ext .js,.jsx,.ts,.tsx",
    "test": "jest --passWithNoTests"
  },
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">1%",
      "last 2 versions",
      "not dead",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.17.12",
    "@nciocpl/eslint-config-vanilla-js": "file:local_packages/nciocpl-eslint-config-vanilla-js-0.8.0.tgz",
    "@nciocpl/eslint-config-vanilla-ts": "file:local_packages/nciocpl-eslint-config-vanilla-ts-0.8.0.tgz",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^27.5.2",
    "@types/jquery": "^3.5.14",
    "@types/jqueryui": "^1.12.16",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^7.32.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "glob": "^8.0.3",
    "jest": "^27.4.7",
    "mini-css-extract-plugin": "^2.5.2",
    "mkdirp": "^1.0.4",
    "nock": "^13.2.7",
    "postcss-loader": "^6.2.1",
    "postcss-prefix-selector": "^1.15.0",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.4.0",
    "svg-sprite": "^1.5.3",
    "ts-jest": "^27.1.5",
    "typescript": "^4.5.4",
    "vinyl": "^2.2.1",
    "webpack": "^5.66.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@nciocpl/ncids-css": "file:local_packages/nciocpl-ncids-css-0.1.0-beta.11.tgz",
    "@nciocpl/ncids-js": "file:local_packages/nciocpl-ncids-js-0.1.0-beta.11.tgz",
    "axios": "^0.25.0",
    "body-scroll-lock": "^2.7.1",
    "geopattern": "^1.2.3",
    "jquery-touchswipe": "^1.6.19",
    "js-cookie": "^2.2.1",
    "query-string": "^5.1.1",
    "slick-carousel": "^1.8.1",
    "sortabletables-js": "^0.1.6",
    "throttle-debounce": "^2.3.0"
  }
}
