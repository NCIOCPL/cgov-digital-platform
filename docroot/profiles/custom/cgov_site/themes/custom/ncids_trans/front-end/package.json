{
  "name": "ncids_trans",
  "version": "1.0.0",
  "description": "Front-end package for ncids_trans theme.",
  "main": "index.js",
  "scripts": {
    "clean:dist": "rimraf ../dist",
    "clean:legacy-sprite": "rimraf ./build && rimraf ./src/styles/sprites/_svg-sprite-map.scss",
    "clean:all": "npm run clean:dist && npm run clean:legacy-sprite",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "npm run build:legacy-sprite && webpack --mode development",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "npm run build:legacy-sprite && webpack --mode production",
    "prewatch": "npm run clean:dist",
    "watch": "npm run build:legacy-sprite && webpack watch --mode development",
    "prebuild:legacy-sprite": "npm run clean:legacy-sprite",
    "build:legacy-sprite": "svg-sprite --dest ./ --config ./legacy-sprites.config.json ../../cgov/cgov_common/assets/sprites/*.svg",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">1%",
      "last 2 versions",
      "not dead",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "jest": "^27.4.7",
    "mini-css-extract-plugin": "^2.5.2",
    "node-sass": "^7.0.1",
    "postcss-loader": "^6.2.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.4.0",
    "svg-sprite": "^1.5.3",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "body-scroll-lock": "^2.7.1",
    "geopattern": "^1.2.3",
    "jquery-touchswipe": "^1.6.19",
    "js-cookie": "^2.2.1",
    "query-string": "^5.1.1",
    "slick-carousel": "^1.8.1",
    "sortabletables-js": "^0.1.6",
    "throttle-debounce": "^2.3.0"
  }
}
