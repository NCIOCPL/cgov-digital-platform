		[ "@babel/preset-typescript", {
			"jsxPragma": "CgdpJsx.createElement",
			"jsxPragmaFragment": "CgdpJsx.Fragment"
		}]
	],
	"plugins": [
		[
			"@babel/plugin-transform-react-jsx",
