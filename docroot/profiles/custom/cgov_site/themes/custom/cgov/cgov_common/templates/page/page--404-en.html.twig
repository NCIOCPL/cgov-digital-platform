{{ page.notification_banner }}

<header class="push" role="banner">
  <div id="nvcgSlSiteBanner" class="row">
     {{ page.site_banner }}
  </div>
</header>

<div id="page">
  <!-- Global nav/utility bar/language  -->
  <div class="fixedtotop">
    <div class="headroom-area">
      {{ page.language_bar }}
      <!-- UTILITY NAV -->
      <div id="nvcgSlUtilityBar" class="utility-background hide-for-medium-down">
        {{ page.utility_bar }}
      </div>
      <!-- END UTILITY NAV -->
    </div>
    <!-- end "headroom-area" -->
    <!-- BEGIN NAVIGATION -->
    <!-- Begin nav-search bar -->
    <div class="nav-search-bar gradient header">
      <div id="nvcgSlMainNav" class="row">
        <div class="slot-item first-SI">
          <div class="navigation">
            {{ page.mobile_nav }}
            {{ page.main_nav }}
          </div>
        </div>
        {{ page.search_bar }}
      </div>
    </div>
    <!-- End nav-search bar -->
    <!-- END NAVIGATION -->
  </div><!-- END Global nav/utility bar/language -->

  <!-- Begin MAIN CONTENT AREA -->
  <div class="main-content" id="content" tabindex="0">
    <div class="row general-page-body-container collapse">
      <div class="large-12 columns">
        <div class="row">
          {{ page.breadcrumb }}
        </div> <!-- END "row" -->
      </div> <!-- END "large-12 columns" -->
      <div class="row">
        <!--    SECTION NAVIGATION -->
        {% if page.section_nav %}
            {{ page.section_nav }}
        {% endif %}
        <div class="medium-9 columns contentzone has-section-nav" id="main" tabindex="0" role="main">
          <div class="error-page">
            <div class="error-content-english">
              <h1>Page Not Found</h1>
              <p>We can't find the page you're looking for.</p>
              <p>Visit the <a href="/">homepage</a>, browse by <a href="/types" title="">cancer type</a>, or use the search below.</p>
              <p>Have a question? <a href="/contact" title="">Get in touch</a>.</p>
            </div>
            <div class="error-searchbar">
              <!-- Begin Search form -->
              <div id="error-form">
                <form action="/search/results" method="get" id="pageNotFoundSearchForm" name="pageNotFoundSearchForm" data-language="en" _lpchecked="1">
                  <label class="hidden" for="nfKeyword">Search</label>
                  <input class="searchString ui-autocomplete-input" id="nfKeyword" maxlength="255" name="swKeyword" onblur="bSearchBoxBool=false;" onfocus="bSearchBoxBool=true;" value="" autocomplete="off" aria-autocomplete="list" aria-haspopup="true" type="text">
                  <button class="searchSubmit" id="nf-search__button" type="submit">Search<span class="hidden">Search</span></button>
                </form>
              </div>
              <!-- End Search form -->
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

{{ page.footer }}
