  <row id="1090628">
    <langcode>en</langcode>
    <name>RD_Process_Overview</name>
    <field_media_image>https://www.cancer.gov/PublishedContent/Images/images/nano/Process Overview.jpeg</field_media_image>
    <field_override_img_thumbnail>https://www.cancer.gov/PublishedContent/Images/images/nano/RD_Process_overview_thumb.jpg</field_override_img_thumbnail>
