{
    "name": "nciocpl/cgov-digital-platform",
    "description": "Cancer.gov Digital Platform",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "acquia/blt": "^13.7.0",
        "acquia/blt-acsf": "^1.0",
        "acquia/blt-behat": "^1.3.0",
        "acquia/blt-phpcs": "^1.0",
        "acquia/memcache-settings": "^1.2",
        "brianium/paratest": "^6.3",
        "commerceguys/addressing": "^1.4",
        "composer/installers": "^1.9.0",
        "cweagans/composer-patches": "^1.7",
        "dmore/behat-chrome-extension": "^1.4",
        "dmore/chrome-mink-driver": "^2.8",
        "drupal/acquia_connector": "^4.0.0",
        "drupal/acquia_purge": "^1.3",
        "drupal/acsf": "^2.73",
        "drupal/address": "1.11.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/akamai": "4.0.0",
        "drupal/autologout": "^1.4",
        "drupal/ckeditor": "^1.0",
        "drupal/config_perms": "^2.1",
        "drupal/core-composer-scaffold": "9.5.10",
        "drupal/core-recommended": "9.5.10",
        "drupal/crop": "^2.3.0",
        "drupal/ctools": "^4.0.0",
        "drupal/devel": "^5.1.2",
        "drupal/embed": "^1.7",
        "drupal/entity_browser": "^2.8",
        "drupal/entity_embed": "^1.4",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/externalauth": "^2.0",
        "drupal/features": "^5.0.0-alpha1",
        "drupal/file_mdm": "^2.6.0",
        "drupal/image_widget_crop": "^2.4.0",
        "drupal/imagemagick": "^3.4.0",
        "drupal/linkit": "^6.0",
        "drupal/memcache": "^2.5.0",
        "drupal/metatag": "^1.26",
        "drupal/moderation_sidebar": "^1.4",
        "drupal/paragraphs": "^1.15",
        "drupal/paragraphs_asymmetric_translation_widgets": "^1.2",
        "drupal/pathauto": "^1.11",
        "drupal/purge": "^3.4",
        "drupal/redirect": "^1.9",
        "drupal/role_delegation": "^1.2.0",
        "drupal/samlauth": "3.x-dev@dev",
        "drupal/seckit": "^2.0.1",
        "drupal/shield": "^1.7.0",
        "drupal/simple_sitemap": "^4.1",
        "drupal/token": "^1.12",
        "drupal/token_filter": "^2.0",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.1",
        "drupal/viewsreference": "^2.0-beta6",
        "drupal/yaml_content": "^1.0-alpha9",
        "drush/drush": "^11.4.0",
        "eluceo/ical": "^0.15.0",
        "justinrainbow/json-schema": "^v5.2.13",
        "kint-php/kint": "^3.3",
        "robrichards/xmlseclibs": "^3.1",
        "scotteh/php-dom-wrapper": "^2.0.3",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "9.5.10",
        "jangregor/phpstan-prophecy": "^1.0",
        "mglaman/phpstan-drupal": "^1.1",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "acquia/blt": true,
            "acquia/blt-phpcs": true,
            "drupal/core-composer-scaffold": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "scripts/composer/CGovScriptHandler.php",
            "scripts/composer/Hoover.php"
        ],
        "files": [
            "load.environment.php"
        ],
        "psr-4": {
            "Cgov\\": "blt/src/",
            "CgovPlatform\\Tests\\": "docroot/lib/CgovPlatform/Tests/"
        }
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ],
        "cgov-init": [
            "git config --local core.hooksPath scripts/hooks",
            "CGovPlatform\\composer\\CGovScriptHandler::initializeProject"
        ],
        "cgov-clean": [
            "CGovPlatform\\composer\\Hoover::clean",
            "@composer clearcache --ansi"
        ],
        "post-drupal-scaffold-cmd": [
            "patch -p1 < ./patches/drupal-scaffold/gitattributes.patch",
            "cd docroot && patch -p1 <../patches/drupal-scaffold/htaccess.https.patch",
            "cd docroot && patch -p1 <../patches/drupal-scaffold/htaccess.hsts.patch",
            "cd docroot && patch -p1 <../patches/drupal-scaffold/htaccess.acsf.patch",
            "cd docroot && patch -p1 <../patches/drupal-scaffold/default.settings.php.translations.patch"
        ]
    },
    "scripts-descriptions": {
        "cgov-init": "Initializes a fresh clone from github. (Setup local docker env, local blt, install hooks)",
        "cgov-clean": "Clean the cancer.gov project tree."
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "enable-patching": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {
            "acquia/memcache-settings": {
                "4033 : Turn off memcache on ODEs, hack for our issue 4033": "patches/acquia/memcache.settings-hack-for-4033.patch"
            },
            "drupal/core": {
                "2952990 : Missing schema for display_options.defaults.sitename_title": "https://www.drupal.org/files/issues/2018-03-14/views-schema-2952990-2.patch",
                "3048288 : Avoid bogus MethodNotAllowedException; see https://github.com/NCIOCPL/cgov-digital-platform/issues/1716": "https://www.drupal.org/files/issues/2019-04-15/route-method-exception-path-fix-3048288.patch",
                "763380  : Do not use \\Drupal\\taxonomy\\TermStorageInterface::loadTree() in \\Drupal\\taxonomy\\Form\\OverviewTerms::buildForm()": "https://www.drupal.org/files/issues/2019-07-08/763380-61.patch",
                "3150294 : (Unofficial Patch) New translations should use initial workflow state" : "patches/drupal/workflow-hack-for-3150294.patch",
                "1308152 : Add stream wrappers to access extension files": "https://www.drupal.org/files/issues/2021-12-15/1308152-395.patch",
                "2971390 : Make exposure of translation meta fields conditional" : "https://www.drupal.org/files/issues/2021-10-29/2971390-62.patch",
                "3359511 : Regression missing menu active trail" : "https://www.drupal.org/files/issues/2023-05-10/3359511-revert-3277784-2.patch",
                "3322743 : FieldConfig-loadByName-return-type-issue" : "patches/drupal/3322743-FieldConfig-loadByName-return-type-issue.patch"
            },
            "drupal/features": {
                "2834130 : Updates Features Diff to Include Alters": "https://www.drupal.org/files/issues/2019-06-09/features-detect-overrides-update-2834130-4-D8.patch"
            },
            "drupal/page_manager": {
                "2820218 : Page manager does not respect existing route defaults for title callbacks": "https://www.drupal.org/files/issues/2018-03-21/2820218-50.patch",
                "2876880 : page_variant entity type does not exist when installing or enabling": "https://www.drupal.org/files/issues/2876880-page-varient-cache-2.patch",
                "3257213 : Route enhacers called multiple times, parameter upcasting / convertion lost": "https://www.drupal.org/files/issues/2022-02-21/3257213-7.patch"
            },
            "drupal/paragraphs": {
                "3331203 : Core 9.5.0 causing broken-looking styling (Legacy widget, with Seven)": "https://www.drupal.org/files/issues/2023-02-22/3331203-11.patch"
            },
            "drupal/purge": {
                "3094343 : Garbled purge tag IDs": "https://www.drupal.org/files/issues/2023-09-07/purge-3094343-15.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "./docroot"
            },
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes",
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            },
            "file-mapping": {
                "[web-root]/robots.txt": false
            }
        },
        "composer-exit-on-patch-failure": true
    }
}
